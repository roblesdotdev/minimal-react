{
  "name": "minimal-react",
  "version": "1.0.0",
  "description": "A minimal react-router stack",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "react-router dev",
    "build": "react-router build",
    "start": "NODE_ENV=production react-router-serve ./build/server/index.js",
    "prettier": "prettier \"**/*.+(js|jsx|css|ts|tsx|cjs|mjs)\"",
    "format": "pnpm prettier --list-different",
    "format:write": "pnpm prettier --write",
    "typecheck": "tsc",
    "lint": "eslint .",
    "prepare": "husky",
    "validate": "run-p typecheck lint format"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roblesdotdev/minimal-react.git"
  },
  "keywords": [
    "remix"
  ],
  "author": "roblesdotdev",
  "license": "MIT",
  "dependencies": {
    "@react-router/node": "^7.0.0-pre.0",
    "@react-router/serve": "^7.0.0-pre.0",
    "isbot": "^5.1.17",
    "react": "19.0.0-rc-1460d67c-20241003",
    "react-dom": "19.0.0-rc-1460d67c-20241003",
    "react-router": "^7.0.0-pre.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.0",
    "@react-router/dev": "^7.0.0-pre.0",
    "@react-router/remix-config-routes-adapter": "0.0.0-nightly-bf7ecb711-20240911",
    "@tailwindcss/vite": "^4.0.0-alpha.26",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.12.0",
    "eslint-plugin-import-x": "^4.3.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^15.10.0",
    "husky": "^9.1.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.40",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "remix-flat-routes": "^0.6.5",
    "tailwindcss": "^4.0.0-alpha.26",
    "typescript": "~5.5.4",
    "typescript-eslint": "^8.8.1",
    "vite": "^5.3.5",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "overrides": {
    "@types/react": "$@types/react",
    "@types/react-dom": "$@types/react-dom",
    "react": "$react",
    "react-dom": "$react-dom"
  },
  "packageManager": "pnpm@9.12.0"
}
